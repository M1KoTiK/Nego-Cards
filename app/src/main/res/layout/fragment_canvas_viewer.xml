<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mainConstraint_qrcViewer"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <m1k.kotik.negocards.data.canvas_qrc.canvas_view.canvas_editors.CanvasView
        android:id="@+id/canvasViewInCanvasViewer"
        android:layout_width="343dp"
        android:layout_height="0dp"
        android:layout_marginTop="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"></m1k.kotik.negocards.data.canvas_qrc.canvas_view.canvas_editors.CanvasView>

    <ImageView
        android:id="@+id/imageView15"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="10dp"
        android:background="@drawable/rounded_square_15"
        android:padding="7dp"
        app:layout_constraintStart_toStartOf="@+id/canvasViewInCanvasViewer"
        app:layout_constraintTop_toBottomOf="@+id/canvasViewInCanvasViewer"
        app:srcCompat="@drawable/download" />

    <ImageView
        android:id="@+id/imageView17"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginStart="35dp"
        android:background="@drawable/rounded_square_15"
        android:padding="3dp"
        app:layout_constraintStart_toEndOf="@+id/imageView15"
        app:layout_constraintTop_toTopOf="@+id/imageView15"
        app:srcCompat="@drawable/share_image_icon" />

    <androidx.recyclerview.widget.RecyclerView
        android:layout_width="409dp"
        android:layout_height="354dp"
        android:layout_marginTop="15dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView15"
        tools:layout_editor_absoluteX="1dp" />

</androidx.constraintlayout.widget.ConstraintLayout>